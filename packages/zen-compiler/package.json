{
  "name": "@sylphx/zen-compiler",
  "version": "0.1.0",
  "description": "Automatic computed inlining optimization for Zen - proven +68% performance improvement",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "USAGE.md",
    "QUICK_START.md"
  ],
  "scripts": {
    "build": "bunup src/index.ts --format esm,cjs --dts --target node --minify",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "zen",
    "reactive",
    "compiler",
    "optimization",
    "babel",
    "inlining",
    "performance",
    "ast",
    "transform",
    "typescript",
    "plugin"
  ],
  "author": "Sylphx <hi@sylphx.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/zen.git",
    "directory": "packages/zen-compiler"
  },
  "bugs": {
    "url": "https://github.com/SylphxAI/zen/issues"
  },
  "homepage": "https://github.com/SylphxAI/zen/tree/main/packages/zen-compiler#readme",
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "dependencies": {
    "@babel/types": "^7.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/parser": "^7.26.0",
    "@babel/preset-typescript": "^7.28.5",
    "bunup": "^0.15.13",
    "typescript": "^5.8.3"
  }
}
